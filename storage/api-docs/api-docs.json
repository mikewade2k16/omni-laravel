    "components": {
        "schemas": {
            "StoreCampaignRequest": {
                "required": [
                    "client_id",
                    "name",
                    "start_date",
                    "end_date"
                ],
                "properties": {
                    "client_id": {
                        "type": "integer",
                        "example": 1
                    },
                    "name": {
                        "type": "string",
                        "example": "Campanha de Verão"
                    },
                    "description": {
                        "type": "string",
                        "example": "Descrição da campanha de verão."
                    },
                    "start_date": {
                        "type": "string",
                        "format": "date",
                        "example": "2025-01-30"
                    },
                    "end_date": {
                        "type": "string",
                        "format": "date",
                        "example": "2025-02-28"
                    },
                    "status": {
                        "type": "string",
                        "example": "active"
                    },
                    "channels": {
                        "type": "array",
                        "items": {
                            "type": "string"
                        },
                        "example": [
                            "facebook",
                            "instagram"
                        ]
                    }
                },
                "type": "object"
            },
            "UpdateCampaignRequest": {
                "properties": {
                    "name": {
                        "type": "string",
                        "example": "Nova Campanha de Verão"
                    }
                },
                "type": "object"
            },
            "StoreClientRequest": {
                "title": "Store Client Request",
                "required": [
                    "nome",
                    "email",
                    "cpf",
                    "user_id"
                ],
                "properties": {
                    "nome": {
                        "type": "string",
                        "example": "João da Silva"
                    },
                    "email": {
                        "type": "string",
                        "format": "email",
                        "example": "joao.silva@example.com"
                    },
                    "cpf": {
                        "type": "string",
                        "example": "123.456.789-10"
                    },
                    "user_id": {
                        "description": "ID do usuário responsável",
                        "type": "integer",
                        "example": 1
                    }
                },
                "type": "object"
            },
            "UpdateClientRequest": {
                "title": "Update Client Request",
                "properties": {
                    "nome": {
                        "type": "string",
                        "example": "João da Silva Santos"
                    },
                    "email": {
